{
    "name": "lancerhe/laravel-admin",
    "description": "The Laravel Admin.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Lancer He",
            "email": "lancer.he@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "4.4.1",
        "laravel/framework": "5.6.40",
        "laravel/tinker": "^1.0",
        "jeroennoten/laravel-adminlte": "v1.27.2",
        "awobaz/compoships": "2.1",
        "barryvdh/laravel-debugbar": "3.4.2",
        "lancerhe/laravel-rbac": "1.0.1",
        "lab404/laravel-impersonate": "1.2.0",
        "predis/predis": "v1.1.7"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
